{	//pager注册
	"pages": [ //pages数组中第一项表示应用启动页（）
		//首页
		{
			"path": "pages/index/index",
			"style": {
				//"navigationBarTitleText": "uni-app"
				"app-plus": {
					//顶部导航栏
					"titleNView": {
						//搜索框
						"searchInput": {
							"align": "center", //在导航栏的位置
							"backgroundColor": "#F7F7F7", //搜索框背景颜色
							"borderRadius": "4upx", //搜索框圆角
							"placeholder": "搜索:", //提示文字
							"placeholderColor": "#CCCCCC" //提示文本颜色
						},
						//按钮
						"buttons": [
							//左边
							{
								"color": "#FF9619", //字体颜色
								"colorPressed": "#BBBBBB", //按下状态的按钮颜色
								"fontSize": "30px", //字体大小
								"float": "left", //浮动到左边
								"fontSrc": "./static/icon.ttf", //按钮文字使用的字体文件路径
								"text": "\ue609" //图片文字编码
							},
							//右边
							{
								"color": "black", //字体颜色
								"colorPressed": "#BBBBBB", //按下状态的按钮颜色
								"fontSize": "30px", //字体大小
								"float": "right", //浮动到左边
								"fontSrc": "./static/icon.ttf", //按钮文字使用的字体文件路径
								"text": "\ue653" //图片文字编码	
							}
						]
					}
				}
			}
		},
       {
            "path" : "pages/me/me",
            "style" : {
				"navigationBarTitleText": "我"	
			}
        }
        ,{
            "path" : "pages/paper/paper",
            "style" : {}
        }
        ,{
            "path" : "pages/news/news",
            "style" : {}
        }
        ,{
            "path" : "pages/search/search",
            "style" : {
				//"navigationBarTitleText": "uni-app"
				"app-plus": {
					"bounce":"none",			//页面回弹效果隐藏
					"scrollIndicator":"none",   //浮动条隐藏
					//顶部导航栏
					"titleNView": {
						"autoBackButton" : "false",
						//搜索框
						"searchInput": {
							"align": "left", //在导航栏的位置
							"backgroundColor": "#F7F7F7", //搜索框背景颜色
							"borderRadius": "8upx", //搜索框圆角
							"disabled":"false",
							"placeholder": "搜索:", //提示文字
							"placeholderColor": "#CCCCCC" //提示文本颜色
						},
						//按钮
						"buttons": [
							//右边
							{
								"color": "black", //字体颜色
								"colorPressed": "#FFDF34", //按下状态的按钮颜色
								"fontSize": "22px", //字体大小
								"float": "right", //浮动到左边
								"text": "取消" //图片文字编码	
							}
						]
					}
				}
			}
        }
    ],
	//全局配置
	"globalStyle": {
		"navigationBarTextStyle": "black",			//文字颜色
		"navigationBarTitleText": "111的页面",		//文字内容
		"navigationBarBackgroundColor":"#F8F8F8",	//导航条背景颜色
		"backgroundColor": "#F8F8F8"				//页面背景颜色
	},
	//底部
	"tabBar":{
		"backgroundColor":"#FFFFFF",
		"color":"#000000",
		"selectedColor":"#ffff7f",
		"height":"70upx",
		"fontSize":"16px",
		"iconWidth":"35px",
		"list":[
			{
				"pagePath":"pages/index/index",
				"iconPath":"static/tabbar/home.png",
				"selectedIconPath":"static/tabbar/homeed.png",
				"text":"主页"
			},
			{
				"pagePath":"pages/paper/paper",
				"iconPath":"static/tabbar/ling.png",
				"selectedIconPath":"static/tabbar/linged.png",
				"text":"动态"
			},
			{
				"pagePath":"pages/news/news",
				"iconPath":"static/tabbar/news.png",
				"selectedIconPath":"static/tabbar/newsed.png",
				"text":"新闻"
			},
			{
				"pagePath":"pages/me/me",
				"iconPath":"static/tabbar/me.png",
				"selectedIconPath":"static/tabbar/meed.png",
				"text":"我的"
			}
		]
	}
}


